@model cw1.Models.Petition

<tr>
    <td>
        <h1 class="text-primary">@Model.Title</h1><hr />
        <p>@Model.Description</p><br />
        <button class="signBtn" id="@Model.Id">Sign</button>
        <input id="signSubmit" type="submit" value="Add" />
<td>
        @foreach (var m in Model.Members)
        {
            @m.Name<br />
        }
    </td>
</tr>
<br /><br />