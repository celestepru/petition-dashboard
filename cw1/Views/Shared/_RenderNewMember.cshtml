@model cw1.Models.Member
@using (Html.BeginForm("AddMember", "Login"))
{
<form role="form">
    <div class="form-group">
        <label for="newUsername">Email address</label>
        @Html.TextBoxFor(m => m.Username, new { placeholder = "Username" })
    </div>
    <div class="form-group">
        <label for="newName">Name</label>
        @Html.TextBoxFor(m => m.Name, new { placeholder = "Name" })
    </div>
    <div class="form-group">
        <label for="newSurname">Surname</label>
        @Html.TextBoxFor(m => m.Surname, new { placeholder = "Surname" })
    </div>
    <div class="form-group">
        <label for="newPassword">Password</label>
        @Html.TextBoxFor(m => m.Password, new { placeholder = "Password" })
    </div>
    <button id="signupSubmit" type="submit" class="btn btn-primary">Sign up</button>
</form>
}